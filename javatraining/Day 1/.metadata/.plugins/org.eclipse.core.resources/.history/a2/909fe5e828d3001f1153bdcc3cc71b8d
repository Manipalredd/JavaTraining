package com.java.agent.model;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.Id;
import javax.persistence.Table;

@Entity
@Table(name = "Agent")
public class Agent {
 
    @Id
    @Column(name = "AgentID")
    private int agentID;
 
    @Column(name = "Name")
    private String name;
 
    @Column(name = "City")
    private String city;
 
    @Column(name = "Gender")
    private String gender;
 
    @Column(name = "MartialStatus")
    private int martialStatus;
 
    @Column(name = "Premium")
    private double premium;
}